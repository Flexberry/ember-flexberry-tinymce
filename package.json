{
  "name": "ember-flexberry-tinymce",
  "version": "0.1.0",
  "description": "Ember Flexberry Tinymce Addon",
  "keywords": [
    "ember-addon",
    "flexberry"
  ],
  "repository": "",
  "license": "MIT",
  "author": {
    "name": "Flexberry Team",
    "email": "mail@flexberry.net",
    "url": "http://flexberry.net/"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember try:each",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --quiet",
    "*.hbs": "ember-template-lint --config '{ \"extends\": [\"recommended\", \"stylistic\"], \"rules\": { \"no-bare-strings\": true } }'"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-funnel": "^3.0.8",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.1.4",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^0.2.23",
    "eslint": "^7.13.0",
    "eslint-plugin-ember": "^9.6.0",
    "eslint-plugin-hbs": "1.0.0",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^5.0.0",
    "eslint-plugin-todo-errors": "^0.3.0",
    "loader.js": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "tinymce": "^6"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
